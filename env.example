# Google Search Trends API Project - Environment Variables Template
# Copy this file to .env and fill in your actual values

# API Configuration
API_CLIENT=pytrends
LANGUAGE=en-US
TIMEZONE=360
RETRIES=3
TIMEOUT=30

# Google API Keys (if using official Google Trends API)
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Alternative API Keys (optional)
SERPAPI_KEY=your_serpapi_key_here
RAPIDAPI_KEY=your_rapidapi_key_here
VALUE_SERP_API_KEY=your_valueserp_api_key_here

# Database Configuration (optional)
DATABASE_URL=sqlite:///data/trends.db
REDIS_URL=redis://localhost:6379

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/trends.log

# Export Configuration
EXPORT_FORMAT=json
EXPORT_DIR=data/exports

# Visualization Configuration
CHART_STYLE=default
CHART_COLOR_PALETTE=husl
FIGURE_SIZE=12,8

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_PERIOD=3600

# Cache Configuration
CACHE_ENABLED=true
CACHE_TTL=3600 